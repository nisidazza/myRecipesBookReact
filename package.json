{
  "name": "my-recipes-book-react",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "server": "node server",
    "postinstall": "knex migrate:latest & knex seed:run",
    "build": "webpack",
    "start": "node server",
    "dev": "npm-run-all --parallel watch:build watch:server",
    "debug": "node --nolazy --inspect-brk=9229 server",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon --nolazy --inspect=9229 server",
    "test": "jest tests",
    "test:dev": "jest --verbose tests",
    "test:dev:watch": "npm run test:dev -- --watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nisidazza/myRecipesBookReact.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nisidazza/myRecipesBookReact/issues"
  },
  "homepage": "https://github.com/nisidazza/myRecipesBookReact#readme",
  "dependencies": {
    "authenticare": "^0.4.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.20.5",
    "pg": "^7.16.1",
    "superagent": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "sqlite3": "^4.1.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-inject-environment-variables": "^1.0.0"
  }
